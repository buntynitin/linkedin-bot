{
    "manifest_version": 3,

    "name": "linkedin-request",
    "description": "Automate linkedin request",
    "version": "1.0",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],

    "action": {
        "default_title": "linkedin-request",
        "default_popup": "popup.html"
    },

    "icons": {
        "128": "logo.png"
    }
}